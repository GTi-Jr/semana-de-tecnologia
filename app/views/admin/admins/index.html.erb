<p id="notice"><%= notice %></p>
<div class="row">
  <div class="col-sm-12">
    <div class="card-box table-responsive">
      <h4 class="header-title m-t-0 m-b-30">Admin Admins</h4>
      <div class="row">
        <div class="col-sm-6">
          <div class="m-b-30">
            <%= link_to(new_admin_admin_path, class:'btn btn-primary waves-effect waves-light') do %>
            New Admin <i class="fa fa-plus"></i>
            <% end %>
          </div>
        </div>
      </div>
      <div class="responsive">
        <table class="table table-striped" id="datatable">
          <thead>
            <tr>
                            <th>Email</th>
                            <th data-orderable="false"></th>
            </tr>
          </thead>
          <tbody>
            <% @admin_admins.each do |admin_admin| %>
              <% if admin_admin != current_admin %>
                <tr>
                  <td><%= admin_admin.email %></td>
                  <td class="actions pull-right">
                    <div class="btn-group m-b-10">
                  <%= link_to 'Mostrar', admin_admin_path(admin_admin), class: 'btn btn-rounded btn-primary waves-effect' %>
                  <%= link_to 'Editar', edit_admin_admin_path(admin_admin), class: 'btn btn-rounded btn-primary waves-effect' %>
                  <%= link_to 'Deletar', admin_admin_path(admin_admin), method: :delete, data: { confirm: 'Tem certeza?' }, class: 'btn btn-rounded btn-danger waves-effect' %>
                </div>
                  </td>
                </tr>
              <% end %>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
    </div><!-- end col -->
  </div>
</div>