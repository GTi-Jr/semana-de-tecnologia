<h4>Sign up</h4>

<%= form_for(resource, as: resource_name, html: {class: 'form-horizontal m-t-20'}, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>


<div class="form-group ">
    <div class="col-xs-12">
        <%= f.label :name  %><br />
        <%= f.text_field :name, autofocus: true, class: "form-control" %>
    </div>
</div>

<div class="form-group ">
    <div class="col-xs-12">
        <%= f.label :course  %><br />
        <%= f.text_field :course, autofocus: true, class: "form-control" %>
    </div>
</div>

<div class="form-group ">
    <div class="col-xs-12">
        <%= f.label :univesity  %><br />
        <%= f.text_field :university, autofocus: true, class: "form-control" %>
    </div>
</div>  

<div class="form-group ">
    <div class="col-xs-12">
        <%= f.label :semester  %><br />
        <%= f.number_field :semester,in:1..12, autofocus: true, class: "form-control" %>
    </div>
</div>   

<div class="form-group ">
    <div class="col-xs-12">
        <%= f.label :birthday  %><br />
        <%= f.date_field :birthday, autofocus: true, class: "form-control" %>
    </div>
</div>  

<div class="form-group ">
    <div class="col-xs-12">
        <%= f.label :email  %><br />
        <%= f.email_field :email, autofocus: true, class: "form-control" %>
    </div>
</div>  

<div class="form-group ">
    <div class="col-xs-12">
       <%= f.label :password %><br />
       <%= f.password_field :password, autocomplete: "off", class: "form-control" %>
    </div>
</div> 

<div class="form-group ">
    <div class="col-xs-12">
       <%= f.label :password_confirmation %><br />
       <%= f.password_field :password_confirmation, autocomplete: "off",class: "form-control" %>
    </div>
</div> 

<div class="form-group text-center m-t-30">
    <div class="col-xs-12">
        <%= button_tag "cadastrar!", class:  "btn btn-custom btn-bordred btn-block waves-effect waves-light", type:"submit"  %>
    </div>
</div>
  

  
<% end %>


