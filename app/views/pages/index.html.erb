<h1>Todas as Semanas</h1>
<div class="row">
  <% @weeks.each do |week| %>
  <div class="col-lg-4 col-md-6">
    <%= link_to root_url(subdomain: week.subdomain) do %>
    <div class="card-box widget-user">
      <div>
      <%= image_tag("#{week.subdomain}.png", class:"img-responsive img-circle")%>
        <div class="wid-u-info">
          <h4 class="m-t-0 m-b-5 font-600"><%= week.name %></h4>
          <p class="text-muted m-b-5 font-13">coderthemes@gmail.com</p>
          <small class="text-warning"><b>12/06 - 20/06</b></small>
        </div>
      </div>
    </div>
    <% end %>
  </div>
  <% end %>
</div>