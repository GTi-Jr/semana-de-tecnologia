      <h1 class="visible-xs"><%= current_week[:sigla] %></h1>
    </div>
    <br>
   <div class="row " style="margin-top: 100px;">

    <%= link_to new_user_registration_path, class:'col s6 l6 m6 btn margin btn-dark btn-lg' do %>
      Inscreva-se
    <% end %>
  

    <%= link_to new_user_session_path, class:'col s6 l6 m6 btn btn-dark btn-lg' do %>
      Acessar sua conta
    <% end %>
  
    </div>
    <br>
   
  </div>
</header>
